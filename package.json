{
	"name": "berry-picker-tracker",
	"version": "1.0.0",
	"main": "./index.js",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"eject": "expo eject",
		"prepare": "husky install",
		"test": "jest"
	},
	"jest": {
		"preset": "jest-expo",
		"transform": {
			"^.+\\.jsx?$": "babel-jest"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-router-native)"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/setup-tests.ts"
		]
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "~1.17.12",
		"@react-native-community/netinfo": "9.3.7",
		"@reduxjs/toolkit": "^1.9.3",
		"crypto-es": "^1.2.7",
		"dotenv": "^16.0.3",
		"expo": "^48.0.8",
		"expo-build-properties": "~0.5.1",
		"expo-cellular": "~5.1.1",
		"expo-dev-client": "~2.1.6",
		"expo-linking": "~4.0.1",
		"expo-location": "~15.1.1",
		"expo-secure-store": "~12.1.1",
		"expo-sharing": "~11.2.2",
		"expo-status-bar": "~1.4.4",
		"expo-task-manager": "~11.1.1",
		"moment": "^2.29.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "^0.71.4",
		"react-native-maps": "^1.4.0",
		"react-native-modal-selector": "^2.1.2",
		"react-native-settings-screen": "^2.2.1",
		"react-native-share": "^8.2.1",
		"react-native-vector-icons": "^9.2.0",
		"react-native-web": "~0.18.12",
		"react-redux": "^8.0.5",
		"react-router-native": "^6.9.0",
		"redux": "^4.2.1",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.3",
		"@testing-library/jest-native": "^5.4.2",
		"@testing-library/react-native": "^12.0.0",
		"@types/jest": "^29.5.0",
		"@types/react": "~18.0.28",
		"@types/react-native": "^0.71.4",
		"@types/react-native-vector-icons": "^6.4.13",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-react": "^7.32.2",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"jest-expo": "^48.0.2",
		"lint-staged": "^13.2.0",
		"npm-check-updates": "^16.7.13",
		"prettier": "^2.8.5",
		"react-test-renderer": "^18.2.0",
		"typescript": "^5.0.2"
	},
	"private": true,
	"lint-staged": {
		"*": "prettier --ignore-unknown --write src",
		"*.{ts, tsx}": "eslint --fix --max-warnings=0 src"
	}
}
